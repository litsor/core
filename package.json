{
  "name": "litsor",
  "version": "0.0.0-development",
  "description": "",
  "main": "litsor.js",
  "scripts": {
    "test": "mocha -b -t 5s core/test/*.js",
    "coverage": "nyc mocha -b -t 5s core/test/*.js",
    "semantic-release": "semantic-release",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "semantic-commit": "commit"
  },
  "author": "",
  "license": "ISC",
  "xo": {
    "esnext": true,
    "space": 2,
    "rules": {
      "space-before-function-paren": 0,
      "max-params": [
        "error",
        5
      ],
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/prompt-cli": "^7.2.0",
    "husky": "^1.1.1",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0",
    "semantic-release": "^15.9.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/litsor/core.git"
  },
  "release": {
    "publish": [
      "@semantic-release/github"
    ]
  }
}
